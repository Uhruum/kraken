{
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "nodemon -w src -e ts -r tsconfig-paths/register --exec ts-node ./src/index.ts"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node src/index.ts"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "fast-xml-parser": "^4.1.3",
    "inversify": "^6.0.1",
    "inversify-express-utils": "^6.4.3",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.15.7",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.22",
    "typescript": "^5.0.2"
  }
}
